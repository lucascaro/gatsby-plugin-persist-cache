  },
  "homepage": "https://github.com/lucascaro/gatsby-plugin-persist-cache/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucascaro/gatsby-plugin-persist-cache.git"
  },
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^9.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/fs-extra": "^9.0.9",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gatsby": "^2.32.11",
    "prettier": "^2.0.5",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0"
  }
